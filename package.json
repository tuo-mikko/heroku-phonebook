{
  "name": "heroku-phonebook",
  "version": "0.0.1",
  "description": "Learning backend",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js",
